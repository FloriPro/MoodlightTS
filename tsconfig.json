{
  "compilerOptions": {
    "allowJs": true,
    "outDir": "out",
    "sourceMap": true,
    "charset": "utf8",
    "target": "es6",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": false,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
  },
  "exclude": ["out/"]
}